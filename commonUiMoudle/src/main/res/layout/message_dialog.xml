<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/rl_message_view"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_message_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/qb_px_110"
        android:layout_marginRight="@dimen/qb_px_110"
        android:gravity="center"
        android:layout_marginTop="@dimen/qb_px_80"
        android:layout_marginBottom="@dimen/qb_px_20"
        android:lineSpacingExtra="@dimen/qb_px_8"
        android:textColor="#333333"
        android:textSize="@dimen/qb_px_44"
        tools:context=".dialog.MessageDialog"
        tools:text="内容" />
</RelativeLayout>